<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Filtragram" %></title>

    <%= stylesheet_link_tag "normalize",  "bootstrap.min", "all" %>
    <%= javascript_include_tag  "all", "bootstrap.min" %>
    <link href='http://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    
  </head>
  <nav class="navbar navbar-default">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <a class="navbar-brand" href="#"><span class="fa fa-instagram"></span> Filtragram</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      
    </div><!-- /.container-fluid -->
  </nav>
  

  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
